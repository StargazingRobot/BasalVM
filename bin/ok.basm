

    copy 4, ax
    
    disp '=', char
    call START
    disp '=', char

    exit

:START
    sub 1, ax
    jump OK if ZRO

    call NOTOK

    jump START
    ret

:OK
    disp 'o', char
    disp 'k', char
    disp 10, char

    exit

:NOTOK
    disp 'N', char
    disp 'o', char
    disp 10, char
    ret
